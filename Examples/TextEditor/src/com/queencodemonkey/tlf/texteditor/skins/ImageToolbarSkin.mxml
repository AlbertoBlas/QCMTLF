<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<!-- Host component -->
	<fx:Metadata>
		[HostComponent("com.queencodemonkey.tlf.texteditor.components.ImageToolbar")]
	</fx:Metadata>
	
	<s:Rect id="borderRectangle"
			width="100%" height="100%">
		<s:stroke>
			<s:SolidColorStroke color="0x666666"
								weight="1"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0xCCCCCC"/>
		</s:fill>
	</s:Rect>
	
	<s:List id="imageList"
			width="100%" height="100%"
			skinClass="com.queencodemonkey.tlf.texteditor.skins.ToolbarVerticalListSkin"/>
	
</s:Skin>
